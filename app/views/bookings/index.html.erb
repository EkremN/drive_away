    <h1>All Bookings</h1>
<div class="container">
  <div class="row" style="width:100%">
    <% @bookings.each do |booking| %>
      <div class="col-sm-3">
        <h2><%= link_to booking.car.title, booking_path(booking) %></h2>
        <h2><%= booking.user.email %></h2>
        <h2><%= booking.start_time %></h2>
        <h2><%= booking.end_time %></h2>
      </div>
    <% end %>
  </div>
</div>
<p><%= link_to "Back to car list", cars_path %></p>
