<div class="container">
  <div class="row">
    <div class="col-sm-6">
      <h1>Hello</h1>
      <% @cars.each do |car| %>
      <ul>
        <li><%= link_to car.brand, car_path(car) %></li>
        <li><%= car.brand %></li>
        <li><%= car.model %></li>
        <li><%= car.year %></li>
        <li><%= car.price %></li>
        <li><%= car.description %></li>
        <li><%= car.photo %></li>
        <li><%= car.address %></li>
        <li><%= car.city%></li>
      </ul>
      <% end %>
    </div>
  </div>
</div>
<h1><%= link_to "Add a new car", new_car_path %></h1>
